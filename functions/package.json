{
  "functions": {
    "source": " functions",
    "runtime": "nodejs22",
    "codebase": "default",
    "defaultRegion": "europe-west1",
    "engines": {
      "node": "22"
    },
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "*.local"
    ]
  },
  "name": "saunastay-functions",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "engines": {
    "node": "22"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "firebase-admin": "^13.4.0",
    "firebase-functions": "^6.3.2",
    "stripe": "^18.2.0"
  },
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "emulators": {
    "auth": {
      "host": "127.0.0.1",
      "port": 9099
    },
    "firestore": {
      "host": "127.0.0.1",
      "port": 8080
    },
    "functions": {
      "host": "127.0.0.1",
      "port": 5002
    },
    "ui": {
      "host": "127.0.0.1",
      "port": 4001
    },
    "hub": {
      "port": 4401
    }
  }
}
